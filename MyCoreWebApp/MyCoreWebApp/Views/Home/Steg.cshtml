@model MyCoreWebApp.Models.Image
@{
    ViewData["Title"] = "Шифрование";
}
<h1>@ViewData["Title"]</h1>

<form asp-action="Images">
    <p><label>Изображение</label></p>
    <p><input type="file" asp-for="value1" /></p>
    <p><label>Ключ</label></p>
    <p><input type="text" asp-for="value2" /></p>
    <p><input type="submit" value="Encode" asp-for="calulate" /></p>
    <p><label>Результат</label></p>
    <p><input type="file" asp-for="@ViewData["result"]" /></p>
</form>